
<div class="container">
    <div class="row">
          <div class="progress">
            <div class="determinate "id="progressBar" value="0" max="100" style="width: 0%"></div>
           <span class="sr-only"></span>
   </div>
        <form id="upload_form" enctype="multipart/form-data" method="post">
        <div class="file-field input-field" >
            <div class="btn">
                <span>File</span>
                <input type="file"  name="image" id="image" accept=".jpg,.jpeg,.png,.gif">
            </div>
         <div class="file-path-wrapper">
                <input class="file-path validate" type="text" placeholder="Upload image" accept=".jpg,.jpeg,.png,.gif" >
            </div>
         </div>
         <div id="image_error" class="error"></div>

        <div class="input-field col s12">
            <input placeholder="Name" id="name" type="text" class="validate" name="name">
        <p id="name_error" class="error"></div>
        </div>
            <input class="btn" type="submit" value="Upload File"  style="width:100%">
            <h3 id="status"></h3>
            <p id="loaded_n_total"></p>
        </form>
    </div>
</div>


   <script type="text/javascript" src="/javascripts/app.js">
